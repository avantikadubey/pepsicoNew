{
	"name": "app",
	"version": "1.0.0",
	"description": "Planogram main application",
	"main": "gatsby-config.js",
	"author": "Pepsico",
	"license": "Private",
	"private": true,
	"scripts": {
		"develop": "npm run setenv:develop --max_old_space_size=4096 && gatsby develop",
		"develop:qa": "npm run setenv:qaindev && gatsby develop",
		"develop:prod": "npm run setenv:prodindev && gatsby develop",
		"build": "npm run setenv:production && gatsby build --prefix-paths",
		"build:prod": "npm run setenv:production && gatsby build --prefix-paths",
		"build:dev": "npm run setenv:development && gatsby build --prefix-paths",
		"build:qa": "npm run setenv:qa && gatsby build --prefix-paths",
		"plop": "plop",
		"setenv:qaindev": "plop qa development",
		"setenv:prodindev": "plop production development",
		"setenv:develop": "plop development development",
		"setenv:production": "plop production production",
		"setenv:development": "plop development production",
		"setenv:qa": "plop qa production",
		"clean": "gatsby clean",
		"scan-local": "sonar-scanner -Dsonar.host.url=http://localhost:9000",
		"scan-prod": "sonar-scanner",
		"sonar-scanner": "sonar-scanner",
		"test": "jest",
		"test:dev": "jest --updateSnapshot",
		"test:watch": "jest --watchAll"
	},
	"jestSonar": {
		"indent": 4,
		"reportFile": "test-reporter.xml",
		"reportPath": "test-results",
		"sonar56x": false
	},
	"keywords": [
		"gatsby",
		"gatsby-app"
	],
	"devDependencies": {
		"babel-jest": "^24.9.0",
		"babel-preset-gatsby": "^0.2.13",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^24.9.0",
		"jest-sonar-reporter": "^2.0.0",
		"redux-saga-test-plan": "^4.0.0-rc.1"
	},
	"dependencies": {
		"@date-io/date-fns": "1.3.11",
		"@dxp/okta": "^1.1.10",
		"@emotion/core": "^10.0.14",
		"@emotion/styled": "^10.0.14",
		"@fortawesome/fontawesome-svg-core": "^1.2.19",
		"@fortawesome/free-brands-svg-icons": "^5.9.0",
		"@fortawesome/free-regular-svg-icons": "^5.9.0",
		"@fortawesome/free-solid-svg-icons": "^5.9.0",
		"@fortawesome/pro-light-svg-icons": "^5.9.0",
		"@fortawesome/pro-regular-svg-icons": "^5.9.0",
		"@fortawesome/pro-solid-svg-icons": "^5.9.0",
		"@fortawesome/react-fontawesome": "^0.1.3",
		"@material-ui/core": "^4.2.0",
		"@material-ui/icons": "^4.5.1",
		"@material-ui/lab": "^4.0.0-alpha.35",
		"@material-ui/pickers": "^3.2.7",
		"@material-ui/styles": "^4.2.0",
		"@okta/okta-signin-widget": "^3.4.2",
		"@planogram/core": "*",
		"@planogram/design-system": "*",
		"@planogram/store": "*",
		"animate.css": "^3.7.2",
		"axios": "^0.18.0",
		"axios-mock-adapter": "^1.17.0",
		"classnames": "^2.2.6",
		"clsx": "^1.0.4",
		"cross-env": "^5.2.0",
		"date-fns": "^2.7.0",
		"dom-helpers": "^5.1.3",
		"emotion-theming": "^10.0.14",
		"file-saver": "^2.0.2",
		"gatsby": "^2.15.12",
		"gatsby-image": "^2.2.18",
		"gatsby-plugin-compile-es6-packages": "^2.1.0",
		"gatsby-plugin-emotion": "^4.0.7",
		"gatsby-plugin-manifest": "^2.2.16",
		"gatsby-plugin-material-ui": "^2.1.4",
		"gatsby-plugin-offline": "^3.0.6",
		"gatsby-plugin-react-helmet": "^3.1.7",
		"gatsby-plugin-sharp": "^2.2.21",
		"gatsby-source-filesystem": "^2.1.22",
		"gatsby-transformer-sharp": "^2.2.14",
		"html-to-react": "^1.4.1",
		"lodash": "^4.17.15",
		"material-table": "^1.57.2",
		"moment-timezone": "^0.5.27",
		"nuka-carousel": "^4.5.12",
		"overlayscrollbars": "^1.9.1",
		"overlayscrollbars-react": "^0.1.0",
