		"react-beautiful-dnd": "^11.0.5",
		"react-dom": "^16.8.6",
		"react-helmet": "^5.2.1",
		"react-number-format": "^4.3.1",
		"react-redux": "^7.1.1",
		"react-swipeable-views": "^0.13.3",
		"react-virtualized": "^9.21.1",
		"redux": "^4.0.4",
		"redux-saga": "^1.0.5",
		"typeface-roboto": "^0.0.54",
		"xlsx": "^0.15.2"
	}
}
